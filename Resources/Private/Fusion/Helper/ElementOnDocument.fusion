// Return number of specific elements found on a document
prototype(Carbon.Condition:ElementOnDocument) < prototype(Neos.Fusion:Value){
    nodeType = null
    node = ${documentNode}
    filterElement = ${'[instanceof ' + this.nodeType + ']'}
    propertyFilter = ''

    backend = true
    live = false

    value = ${(this.backend && node.context.inBackend) || (this.live && node.context.live) || q(this.node).find(this.filterElement + this.propertyFilter).count()}
}
