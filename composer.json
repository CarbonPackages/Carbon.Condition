{
    "name": "carbon/condition",
    "type": "neos-carbon",
    "description":
        "This package provides some fusion helper for making writing conditions (`@if`) easier.",
    "license": "MIT",
    "keywords": ["flow", "neos", "fusion", "helper", "condition", "carbon"],
    "require": {
        "neos/neos": "~3.0 || ~4.0 || ~5.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://jonnitto.ch",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Condition\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.Condition",
        "neos": {
            "package-key": "Carbon.Condition"
        }
    }
}
